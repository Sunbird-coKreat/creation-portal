{
  "name": "src",
  "version": "1.7.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng build --deploy-url dist/ && mv ../dist/index.html ../dist/index.ejs",
    "proxy": "ng serve --proxy-config proxy.config.js",
    "build": "node --max_old_space_size=8096 ./node_modules/@angular/cli/bin/ng build --configuration production --deploy-url dist/",
    "build-dev": "ng build --configuration production --build-optimizer=false --deploy-url dist/",
    "test": "ng test --tags $tags",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -s",
    "test-coverage": "ng test src --code-coverage=true --watch=false --sourceMap=false",
    "desktop-test-coverage": "ng test desktop --watch=false",
    "build-cdn": "ng build --configuration production",
    "build-offline": "ng build --project offline --deploy-url dist/ --c=offline && mv ../dist/index.html ../dist/index.ejs",
    "start-offline": "nodemon  --exec 'npm run build-offline'",
    "build-offline-prod": "ng build --project offline  --c=offlineProduction && mv ../dist/index.html ../dist/index.ejs",
    "inject-cdn-fallback": "gulp inject:cdnFallBack:script",
    "offline-dev": "ng build --project desktop --watch=true --outputPath=<desktop_portal_dist_file>",
    "offline-prod": "ng build --project desktop --configuration production && mv ../dist/index.html ../dist/index.ejs"
  },
  "nodemonConfig": {
    "ext": "png,jpeg,js,ts,html,json,css,scss"
  },
  "engines": {
    "node": ">=8.9"
  },
  "private": true,
